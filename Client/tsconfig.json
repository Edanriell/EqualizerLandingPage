{
	"extends": "astro/tsconfigs/strictest",
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": "./src",
		"paths": {
			"@app/*": ["app/*"],
			"@entities/*": ["entities/*"],
			"@pages/*": ["pages-astro/*"],
			"@widgets/*": ["widgets/*"],
			"@shared/*": ["shared/*"],
			"@public/*": ["../public/*"]
		}
	}
}